{
  "name": "ryanvarick.com",
  "version": "16.0.0",
  "author": "Ryan Varick",
  "description": "Website for Ryan Varick",
  "private": true,
  "repository": "TBD",
  "readme": "README.md",
  "license": "MIT",
  "main": "index.html",
  "scripts": {
    "build:less": "lessc assets/styles/app.less assets/app.css",
    "build:autoprefix": "autoprefixer assets/app.css",
    "build:css": "npm run build:less && npm run build:autoprefix",
    "build:fonts": "rm assets/fonts/* && cp vendor/font-awesome/fonts/* assets/fonts",
    "build:js": "browserify assets/js/app.js -o assets/app.js",
    "build": "npm run build:css && npm run build:fonts && npm run build:js",
    "cleanup:logs": "rm npm-debug.log || true",
    "cleanup:processes": "pkill -f `pwd -P` || true",
    "cleanup": "npm run cleanup:logs && npm run cleanup:processes",
    "open:atom": "open -a 'Atom' . -n",
    "open:chrome": "open -a 'Google Chrome' http://localhost:9090",
    "open:firefox": "open -a 'Firefox' http://localhost:9090",
    "open:simulator": "open /Applications/Xcode.app/Contents/Developer/Applications/iOS\\ Simulator.app",
    "open:sourcetree": "open -a 'SourceTree' . -n",
    "open": "npm run open:atom && npm run open:chrome && npm run open:sourcetree",
    "open:all": "npm run open && npm run open:firefox && npm run open:simulator",
    "open:dev": "npm run open:atom && npm run open:chrome && npm run open:sourcetree",
    "livereload": "live-reload --port 9091 .",
    "serve": "http-server -s -p 9090 .",
    "version": "versiony --to 'bower.json' --from 'package.json'",
    "watch:css": "watch 'clear && npm run build:css' assets/styles/",
    "watch:js": "watch 'clear && npm run build:js' assets/js/",
    "dev": "clear && npm run cleanup && npm run cleanup:processes && parallelshell 'npm run livereload' 'npm run serve' 'npm run watch:css'"
  },
  "devDependencies": {
    "autoprefixer": "^4.0.0",
    "browserify": "^7.0.0",
    "clear": "0.0.1",
    "http-server": "^0.7.4",
    "less": "^2.1.1",
    "live-reload": "^0.2.0",
    "parallelshell": "^1.0.0",
    "versiony-cli": "^1.3.0",
    "watch": "^0.13.0"
  }
}
